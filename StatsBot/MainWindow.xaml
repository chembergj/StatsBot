<Window x:Class="StatsBot.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="900" Width="921" Loaded="Window_Loaded">
    <Grid Margin="0,10,0,0">
        <WebBrowser x:Name="browser" HorizontalAlignment="Left" Height="537" VerticalAlignment="Top" Width="913" Source="http://stats.vatsim.net" Margin="0,115,0,0" LoadCompleted="browser_LoadCompleted"/>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="0,657,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="166" Width="893"/>
        <Button Content="Click when logged in!" HorizontalAlignment="Left" Margin="261,828,0,0" VerticalAlignment="Top" Width="151" Click="Button_Click"/>
        <Label Content="1. Select filename for member input list:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="259"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="294,13,0,0" TextWrapping="Wrap" Text="members.csv" VerticalAlignment="Top" Width="294"/>
        <Button Content="Select" HorizontalAlignment="Left" Margin="620,14,0,0" VerticalAlignment="Top" Width="75"/>
        <Label Content="3. Login on stats.vatsim.net below:" HorizontalAlignment="Left" Margin="10,72,0,0" VerticalAlignment="Top" Width="330"/>

    </Grid>
</Window>
